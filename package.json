{
  "name": "webpackdemo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack --config config/webpack.config.js --mode development",
    "prod": "webpack --config config/webpack.config.js --mode production",
    "start": "webpack-dev-server  --config config/webpack.config.js --mode development  --open",
    "watch": "webpack --config config/webpack.config.js --mode development --watch",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less}'",
    "prettierSingle": "prettier --write"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{css,less}": [
      "prettier --write"
    ],
    "*.{js?(x)}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.37",
    "@types/react-dom": "^16.9.8",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "prettier": "^2.0.5",
    "style-loader": "^1.1.3",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "lint-staged": "^10.2.10",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "yorkie": "^2.0.0"
  }
}
